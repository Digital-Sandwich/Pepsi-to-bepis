{
"manifest_version": 2,
	"name": "Pepsi to bepis",
	"description": "This extension replaces a pages instances of the text 'pepsi' with 'bepis'",
	"version": "1.0",
	"content_scripts": [
	        {
	            "matches": [
	                "*://*/*"
	            ],
	            "js": [
	                "bepisitize.js"
	            ],
	            "run_at": "document_end"
	        }
	    ],
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	}
}